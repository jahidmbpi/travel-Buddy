{
  "name": "backent",
  "version": "1.0.0",
  "prisma": {
    "schema": "./prisma/schema"
  },
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "prisma": "dotenv -e .env -- prisma",
    "postinstall": "prisma generate"
  },
  "main": "src/server.ts",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "6",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "prisma": "6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "nodemon": "^3.1.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
